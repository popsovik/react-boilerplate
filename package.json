{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React webpack boilerplate with PostCSS modules, pages chunking with React Suspense and lazy, lint, prettier, jest setup",
  "scripts": {
    "start": "webpack-dev-server --hot --open",
    "build": "NODE_ENV=production webpack",
    "build-dev": "webpack",
    "analyze": "ANALYZE=1 NODE_ENV=production webpack",
    "clean": "rm -rf dist coverage",
    "lint": "eslint ./src",
    "lint-fix": "eslint --fix ./src",
    "prettier": "prettier -l './src/**/*.{js,css}'",
    "prettier-fix": "prettier --write './src/**/*.{js,css}'",
    "test": "jest",
    "test:watch": "jest --watch test/**",
    "test:coverage": "jest --coverage test/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoilorys/react-boilerplate.git"
  },
  "alias": {
    "react-dom": "@hot-loader/react-dom"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.8.0",
    "react-test-renderer": "^16.8.4",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-bundle-analyzer": "^3.4.1"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.4",
    "classnames": "^2.2.6",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "5.0.0",
    "rxjs": "^6.5.1"
  }
}
